{
  "name": "@vercel/blob",
  "version": "0.2.6",
  "description": "The Vercel Blob JavaScript API client",
  "homepage": "https://vercel.com/blob",
  "repository": {
    "type": "git",
    "url": "https://github.com/vercel/vercel.git",
    "directory": "packages/blob"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "node": "./dist/index.js",
        "default": "./dist/index.js"
      },
      "require": {
        "node": "./dist/index.cjs",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "browser": {
    "undici": "./dist/undici-browser.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "undici": "5.21.2"
  },
  "devDependencies": {
    "tsup": "6.7.0"
  },
  "engines": {
    "node": ">=16.14"
  },
  "scripts": {
    "build": "tsup && cp src/undici-browser.js dist/undici-browser.js",
    "dev": "cp src/undici-browser.js dist/undici-browser.js && tsup --watch --clean=false"
  }
}